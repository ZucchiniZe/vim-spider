--langdef=spider
--langmap=spider:.spider
--regex-spider=/([A-Za-z0-9._$]+)[ \t]*[:=][ \t]*\{/\1/,object/
--regex-spider=/([A-Za-z0-9._$()]+)[ \t]*[:=][ \t]*func[ \t]*\(/\1/,func/
--regex-spider=/func[ \t]+([A-Za-z0-9._$]+)[ \t]*([^)])/\1/,func/
--regex-spider=/([A-Za-z0-9._$]+)[ \t]*[:=][ \t]*\[/\1/,array/
--regex-spider=/([^= ]+)[ \t]*=[ \t]*[^"]'[^']*/\1/,string/
--regex-spider=/([^= ]+)[ \t]*=[ \t]*[^']"[^"]*/\1/,string/
